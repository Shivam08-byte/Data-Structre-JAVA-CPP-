#include <iostream>
using namespace std;

int main() {
    
    int t=0;
    cin>>t;
    int i=0;
    while(i<t){
        int n;
        cin>>n;
        int arr[n];
        int d;
        cin>>d;
        for(int i=0;i<n;i++){
            cin>>arr[i];
        }
        
        for(int i=0;i<n;i++){
            cout<<arr[(i+d)%n]<<" ";
        }
        cout<<endl;
        i++;
    }
	//code
	return 0;
}
